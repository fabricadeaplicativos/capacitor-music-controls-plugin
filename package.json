{
  "name": "capacitor-music-control",
  "version": "2.1.0-beta.0",
  "description": "Media control for capacitor projects",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "CapacitorMusicControl.podspec"
  ],
  "author": "Luiz Eduardo C",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/selcip/capacitor-music-controls-plugin"
  },
  "bugs": {
    "url": "https://github.com/selcip/capacitor-music-controls-plugin/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "devDependencies": {
    "@capacitor/android": "^3.0.0",
    "@capacitor/core": "^3.0.0",
    "@capacitor/docgen": "^0.0.10",
    "@capacitor/ios": "^3.0.0",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.11.0",
    "prettier": "~2.2.0",
    "prettier-plugin-java": "~1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "standard-version": "^9.3.2",
    "swiftlint": "^1.0.1",
    "typescript": "~4.0.3"
  },
  "peerDependencies": {
    "@capacitor/core": "^3.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
